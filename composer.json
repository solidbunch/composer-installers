{
  "name": "solidbunch/composer-installers",
  "type": "composer-plugin",
  "description": "Custom installer for StarterKit package types",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "composer-plugin-api": "^2.0"
  },
  "require-dev": {
    "composer/composer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "SolidBunch\\ComposerInstallers\\": "src/"
    }
  },
  "extra": {
    "class": "SolidBunch\\ComposerInstallers\\Plugin"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
